
.body{
	padding: 0 20rpx;
}
.user-list{
	padding: 20rpx 0;
	border-bottom: 1rpx solid #EEEEEE;
}
.user-list>image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 100%;
	margin-right: 20rpx;
	-webkit-flex-shrink: 0;
	    -ms-flex-negative: 0;
	        flex-shrink: 0;
}
.user-list>view:first-of-type{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.user-list>view:first-of-type>view:first-child{
	font-size: 35rpx;
}
.user-list>view:last-of-type{
	width: 80rpx;
	color: #CCCCCC;
}


